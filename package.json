{
  "name": "notes",
  "version": "1.0.0",
  "private": true,
  "productName": "Notes",
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "electron": "^21.2.0",
    "json-as-xlsx": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start-electron": "electron .",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "build": "electron-packager . NoteIt --platform=win32 --arch=x64",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "npm-watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron-packager": "^17.0.0",
    "npm-watch": "^0.11.0",
    "sass": "^1.53.0"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx",
      "quiet": false,
      "legacyWatch": true,
      "runOnChangeOnly": false
    }
  }
}
